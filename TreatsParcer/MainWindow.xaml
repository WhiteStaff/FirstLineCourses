<Window x:Class="TreatsParcer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TreatsParcer"
        xmlns:generic="clr-namespace:System.Collections.Generic;assembly=mscorlib"
        xmlns:collections="clr-namespace:System.Collections;assembly=mscorlib"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <DataGrid Name="TreatsGrid" Height="409" Margin="10,0" VerticalAlignment="Top" AutoGenerateColumns="True" ItemsSource="{DynamicResource ResourceKey=treats}" Loaded="TreatsGrid_Loaded" MouseDoubleClick="TreatsGrid_MouseDoubleClick">
            <DataGrid.Resources>
                <collections:ArrayList x:Key="treats">
                </collections:ArrayList>
            </DataGrid.Resources>
        </DataGrid>

    </Grid>
</Window>
